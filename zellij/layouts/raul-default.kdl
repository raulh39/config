layout {
  tab name="Main" hide_floating_panes=true {
    pane size=1 borderless=true {
      plugin location="tab-bar"
    }
    pane name="Main"
    pane size=1 borderless=true {
      plugin location="status-bar"
    }
    floating_panes {
      pane { x "10%"; y "10%"; width "80%"; height "80%"; }
    }
  }
}
